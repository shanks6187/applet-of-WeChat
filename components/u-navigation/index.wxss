/* components/u-navigation/index.wxss */
.weui-navigation-bar{--statusHeight:20px;--innerHeight:44px;--innerRight:190rpx;--innerMargin:15px;--btnWidth:88px;--btnHeight:32px}
.weui-navigation-bar.android{--innerHeight:48px;--innerRight:222rpx}
.weui-navigation-bar__placeholder{position:relative;z-index:50;padding-top:var(--statusHeight);height:var(--innerHeight);background:#F8F8F8;opacity:0;visibility:hidden;box-sizing:content-box}

.weui-navigation-bar__inner{position:fixed;top:0;left:0;right:0;z-index:5001;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-align-items:center;align-items:center;padding:0 var(--innerRight);padding-top:var(--statusHeight);height:var(--innerHeight);box-sizing:content-box}
.weui-navigation-bar__inner.navigation-bar--border:after{content:"";position:absolute;left:0;bottom:0;display:block;width:100%;height:1px;background:var(--innerBorder);-webkit-transform-origin:0 0;transform-origin:0 0;-webkit-transform:scaleY(.5);transform:scaleY(.5);pointer-events:none}

.weui-navigation-bar__left{position:relative;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-align-items:center;align-items:center;margin-left:calc(-1 * var(--innerRight));width:var(--innerRight);padding-left:var(--innerMargin);box-sizing:border-box}

.weui-navigation-bar__back{position:relative;display:flex;align-items:center;width:calc(var(--btnWidth)/2);height:var(--btnHeight)}
.weui-navigation-bar__back .bar__btn-image{width:24px;height:24px;}

.weui-navigation-bar__btn{ position:relative;display:flex;align-items:center;border:0 solid #EBEBEB;background-color:rgba(255,255,255,.5);border-radius:99px;box-sizing:border-box}
.weui-navigation-bar__btn:after{content:" ";position:absolute;top:0;left:0;border:inherit;border-width:1px;width:200%;height:200%;-webkit-transform-origin:0 0;transform-origin:0 0;-webkit-transform:scale(.5);transform:scale(.5);border-radius:inherit;pointer-events:none;box-sizing:border-box}
.weui-navigation-bar__btn .bar__btn-item{position:relative;display:flex;justify-content:center;align-items:center;width:calc(var(--btnWidth)/2);height:var(--btnHeight)}
.weui-navigation-bar__btn .bar__btn-item:after{content:"";position:absolute;right:0;top:50%;display:block;width:1px;height:60%;background-color:#EBEBEB;-webkit-transform:scaleX(.5) translateY(-50%);transform:scaleX(.5) translateY(-50%)}
.weui-navigation-bar__btn .bar__btn-item:last-child:after{display:none}
.weui-navigation-bar__btn .bar__btn-item:active{opacity:.5}
.weui-navigation-bar__btn .bar__btn-image{width:30px;height:30px;-webkit-transform:scale(.7);transform:scale(.7)}

.weui-navigation-bar__center{font-size:14px;text-align:center;position:relative;flex:1;display:-webkit-box;display:-webkit-flex;display:flex;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;overflow:visible}
.weui-navigation-bar__center .bar__center-text{margin-top:-2px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;font-size: 34rpx;}
.weui-navigation-bar__center .weui-navigation-bar__loading{flex:0 0 auto;width:20px;height:20px;margin-right:5px;content:" ";animation:loading-animate 1s steps(12, end) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;background-size:100%}
@keyframes loading-animate{
  0%{transform:rotate3d(0,0,1,0deg);}
  100%{transform:rotate3d(0,0,1,360deg);}
}